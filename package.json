{
  "scripts": {
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "app/**/*.{js,jsx,scss,json,yml,css}": [
      "eslint --fix",
      "git add"
    ],
    "{app,test}/**/*.rb": [
      "bundle exec rubocop -a",
      "git add"
    ]
  },
  "engines": {
    "node": "11.x",
    "npm": "6.x",
    "yarn": "1.x"
  },
  "devDependencies": {
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^0.14.3",
    "lint-staged": "^4.1.0",
    "prettier": "^1.6.1",
    "webpack-dev-server": "^3.2.1",
    "node-sass": "^4.13.0"
  },
  "dependencies": {
    "@babel/preset-react": "^7.0.0",
    "@rails/webpacker": "^4.0.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-react": "^6.24.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.3",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.1.0"
  }
